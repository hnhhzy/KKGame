<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <appSettings>
    <add key="port" value="4321"/>
    <!--最大连接数-->
    <add key="numConnections" value="20000"/>
    <!--缓存大小-->
    <add key="receiveBufferSize" value="64"/>
    <!--超时时间-->
    <add key="overtime" value="600"/>
  </appSettings>
</configuration>